<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.credit.diversion.dao.TbannerMapper">

    <resultMap type="com.credit.diversion.model.Tbanner" id="mTbanner" autoMapping="true">
    </resultMap>
    <resultMap type="com.credit.diversion.model.Tbanner" id="mTbannerAll">
        <id column="id" property="id"/>
        <result column="sub_title" property="subTitle"/>
        <result column="title_desc" property="titleDesc"/>
        <result column="point_url" property="pointUrl"/>
        <result column="pic_url" property="picUrl"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
    </resultMap>

    <sql id="mColumnsSql">
        id AS id,
        sub_title AS subTitle,
        title_desc AS titleDesc,
        point_url AS pointUrl,
        pic_url AS picUrl,
        created AS created,
        updated AS updated
    </sql>
</mapper>