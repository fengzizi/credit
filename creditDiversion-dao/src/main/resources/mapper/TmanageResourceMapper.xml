<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.credit.diversion.dao.TmanageResourceMapper">

    <resultMap type="com.credit.diversion.model.TmanageResource" id="mTmanageResource" autoMapping="true">
    </resultMap>
