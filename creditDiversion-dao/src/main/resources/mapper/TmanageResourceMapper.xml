        <result column="resource_order" property="resourceOrder"/>
        <result column="resource_status" property="resourceStatus"/>
        <result column="resource_group" property="resourceGroup"/>
    </resultMap>

    <sql id="mColumnsSql">
        id AS id,
        pid AS pid,
        resource_name AS resourceName,
        resource_url AS resourceUrl,
        resource_type AS resourceType,
        resource_icon AS resourceIcon,
        resource_order AS resourceOrder,
        resource_status AS resourceStatus,
        resource_group AS resourceGroup
    </sql>
</mapper>